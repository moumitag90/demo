<html>
<%=form_for @user do |f|%>
<h2><%=f.label :name%>
<%=f.text_field :name%></h2>
<h2><%=f.label :email%>
<%=f.text_field :email%></h2>
<h2><%=f.label :dept%>
<%=f.text_field :dept%></h2>
<h2><%=f.label :roll_no%>
<%=f.text_field :roll_no%></h2>
<h2><%=f.label :age%>
<%=f.text_field :age%></h2>
<%=f.submit%>
<%end%>
</html>
